{
  "name": "reactdamuonsite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:security": "eslint . --ext .js,.jsx -c eslint.config.js",
    "lint:backend": "eslint server.js *-api.js backend-utils.js",
    "lint:frontend": "eslint src/**/*.{js,jsx}",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "docs": "jsdoc -c jsdoc.json",
    "code-quality": "npm run lint && npm run format:check",
    "code-fix": "npm run lint:fix && npm run format",
    "backend-quality": "npm run lint:backend && npm run test:backend-syntax",
    "backend-security": "npm run lint:security && npm run audit",
    "test:backend-syntax": "node -c server.js && node -c backend-utils.js && node -c blog-api.js && node -c upload-api.js && node -c questionnaire-api.js && node -c resume-api.js",
    "start:dev": "NODE_ENV=development node server.js",
    "start:prod": "NODE_ENV=production node server.js",
    "start:resume-api": "node resume-api.js",
    "logs:view": "tail -f backend-utils.js",
    "logs:errors": "cat logs/error.log 2>/dev/null || echo 'No error logs found'",
    "logs:clear": "rm -rf logs/ 2>/dev/null || echo 'No logs to clear'",
    "health": "curl -s http://localhost:3001/health | json_pp || echo 'Server not running'",
    "audit": "npm audit --audit-level moderate",
    "security-check": "npm run audit && npm run lint:security",
    "preview": "vite preview"
  },
  "dependencies": {
    "@date-io/date-fns-jalali": "^3.2.0",
    "@hello-pangea/dnd": "^18.0.1",
    "axios": "^1.12.2",
    "chart.js": "^4.5.0",
    "cors": "^2.8.5",
    "date-fns-jalali": "^3.2.0-0",
    "dayjs": "^1.11.18",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-validator": "^7.2.1",
    "front-matter": "^4.0.2",
    "hoist-non-react-statics": "^3.3.2",
    "html2canvas": "^1.4.1",
    "jalaliday": "^3.1.1",
    "jspdf": "^3.0.3",
    "lottie-react": "^2.4.1",
    "lucide-react": "^0.545.0",
    "marked": "^16.3.0",
    "moment": "^2.29.4",
    "moment-jalaali": "^0.9.2",
    "multer": "^2.0.2",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-parallax": "^3.5.2",
    "react-router-dom": "^7.9.1",
    "three": "^0.180.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/parser": "^8.44.0",
    "@vitejs/plugin-react": "^5.0.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-security": "^3.0.1",
    "express-rate-limit": "^8.1.0",
    "globals": "^16.3.0",
    "helmet": "^8.1.0",
    "jsdoc": "^4.0.4",
    "prettier": "^3.6.2",
    "vite": "^7.1.2"
  }
}
